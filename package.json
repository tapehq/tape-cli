{
  "name": "tape-cli",
  "version": "0.12.3-0",
  "author": "Edamame Studio @ingmaras @dac09",
  "bin": {
    "tape": "./bin/tape"
  },
  "bugs": "https://github.com/edamameldn/tape-cli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@oclif/plugin-not-found": "^1.2.4",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "@types/command-exists": "^1.2.0",
    "@types/ffmpeg-static": "^3.0.0",
    "@types/ffprobe-static": "^2.0.0",
    "@types/string-similarity": "^3.0.0",
    "adbkit": "^2.11.1",
    "await-exec": "^0.1.2",
    "aws-sdk": "^2.663.0",
    "axios": "^0.19.2",
    "chalk": "^4.0.0",
    "cli-ux": "^5.4.5",
    "clipboardy": "^2.3.0",
    "command-exists": "^1.2.9",
    "decompress": "^4.2.1",
    "decompress-tarxz": "^3.0.0",
    "decompress-unzip": "^4.0.1",
    "ffmpeg-static": "^4.2.6",
    "ffprobe-static": "^3.0.0",
    "filesize": "^6.1.0",
    "graphql-request": "2.1.0-next.1",
    "inquirer": "^7.1.0",
    "inquirer-table-prompt": "^0.2.1",
    "lodash": "^4.17.19",
    "mime-types": "^2.1.27",
    "new-github-issue-url": "^0.2.1",
    "niceware": "^2.0.0",
    "open": "^7.0.4",
    "pretty-bytes": "^5.3.0",
    "string-similarity": "^4.0.2",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/aws-sdk": "^2.7.0",
    "@types/chai": "^4",
    "@types/chalk": "^2.2.0",
    "@types/decompress": "^4.2.3",
    "@types/inquirer": "^6.5.0",
    "@types/mime-types": "^2.1.0",
    "@types/mocha": "^5",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "mocha": "^5",
    "np": "^6.2.3",
    "nyc": "^14",
    "prettier": "2.0.5",
    "prettier-eslint": "^9.0.1",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">10.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://tape.sh",
  "keywords": [
    "oclif",
    "simulator",
    "ios",
    "android",
    "screen record",
    "record",
    "share screen",
    "gif",
    "screenshot"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "tape",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-warn-if-update-available"
    ],
    "warn-if-update-available": {
      "timeoutInDays": 7,
      "message": "<%= config.name %> update available from <%= chalk.greenBright(config.version) %> to <%= chalk.greenBright(latest) %>.",
      "registry": "https://registry.npmjs.org"
    },
    "hooks": {
      "init": [
        "./lib/hooks/init/check-auth"
      ],
      "prerun": "./lib/hooks/prerun/set-adb-envs"
    }
  },
  "repository": "git://github.com/edamameldn/tape-cli.git",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "postinstall": "echo 'ðŸŽ‰  Tape installed! \n Please run `tape config --setup` to get started \n'",
    "test": "mocha --forbid-only 'test/**/*.test.ts'",
    "version": "oclif-dev readme && git add README.md",
    "docs": "oclif-dev readme",
    "release": "np"
  },
  "types": "lib/index.d.ts"
}
